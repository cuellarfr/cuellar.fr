    {% for show in site.shows reversed %}
    <item>
      <title>
        <![CDATA[ {{ show.title }} ]]>
      </title>
      <link>{{ site.url }}{{ show.url }}</link>
      <enclosure url="{{ show.file }}" type="audio/mpeg" length="{{ show.length }}"/>
      <description>
        <![CDATA[ {{ show.description }} ]]>
      </description>
      <pubDate>{{ show.date | date_to_xmlschema }}</pubDate>
      <itunes:duration>{{ show.duration }}</itunes:duration>
      <itunes:episodeType>full</itunes:episodeType>
      <guid>{{ site.url }}{{ show.url }}</guid>
      <itunes:image href="{{ show.image }}"/>
    </item>
